<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.alfi.senangalquran">

    <application
        android:name=".App"
        android:allowBackup="true"
        android:icon="@mipmap/quran"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">

        <activity
        android:name=".presentation.listsurah.ListSurahActivity"
        tools:ignore="InnerclassSeparator" />

        <activity
            android:name=".presentation.splashscreen.SplashscreenActivity"
            tools:ignore="InnerclassSeparator">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>


        <activity android:name=".presentation.lisayat.ListAyatActivity"></activity>
    </application>

</manifest>